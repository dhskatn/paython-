n1= int(input('첫번째 숫자를 입력해주세요:'))#n1에 숫자를 입력 받는다.
n2 = int(input('두번째 숫자를 입력해주세요:'))#n2에 숫자를 입력 받는다.

for i in range(n2,(n1*n2)+1): #n2부터 (n1*n2)+1의 범위까지 반복한다.
    if (i%n1==0 and i%n2==0):#만약 i가 n1으로 나누어 나어지가 0일 경우
                             #그리고 i가 n2로 나누어 나머지가 0이라면 
        print(i) #i를 보여줘라
        break #멈춰라
